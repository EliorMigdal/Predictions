<HBox alignment="CENTER" prefHeight="65.0" prefWidth="168.0" stylesheets="@../../../../../../Styles/Styles.css"
      xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="predictions.controllers.client.results.components.upperscreen.controller.rerunButtonSetController">
   <Button fx:id="rerunSimulationButton" mnemonicParsing="false" onAction="#reRunSimulation" text="Rerun!">
