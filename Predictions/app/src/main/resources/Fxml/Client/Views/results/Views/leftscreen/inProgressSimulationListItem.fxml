   <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@../../../../../../Images/executeImg.png" />
   </ImageView>
   <Label fx:id="simulationID" prefHeight="32.0" prefWidth="117.0">
      <font>
         <Font name="Berlin Sans FB" size="14.0" />
      </font>
   </Label>
   <ImageView fx:id="statusImage" disable="true" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" />
</HBox>
